{
	"name": "@bagsfm/bags-sdk",
	"version": "1.2.5",
	"description": "TypeScript SDK for Bags",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"private": false,
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "tsc",
		"clean": "rm -rf dist",
		"prepublishOnly": "npm run clean && npm run build",
		"dev": "tsc --watch",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --ext .ts --fix",
		"format": "prettier --write \"src/**/*.{ts,js,json}\"",
		"format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
		"prepare": "husky"
	},
	"lint-staged": {
		"src/**/*.{ts,tsx}": "eslint --fix"
	},
	"keywords": [
		"bags",
		"solana",
		"token",
		"token launch",
		"sdk",
		"typescript",
		"memecoin",
		"meteora"
	],
	"author": {
		"name": "Ramyo Bags",
		"email": "ramyo@bags.fm",
		"url": "https://github.com/ramyodev"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/bagsfm/bags-public-api-sdk.git"
	},
	"homepage": "https://bags.fm",
	"bugs": {
		"url": "https://github.com/bagsfm/bags-public-api-sdk/issues"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"dependencies": {
		"@coral-xyz/anchor": "^0.31.1",
		"@meteora-ag/cp-amm-sdk": "1.1.0",
		"@meteora-ag/dynamic-bonding-curve-sdk": "1.3.5",
		"@solana/spl-token": "0.4.13",
		"@solana/web3.js": "1.98.4",
		"axios": "1.11.0",
		"bs58": "6.0.0",
		"form-data": "4.0.4"
	},
	"devDependencies": {
		"@types/bn.js": "5.2.0",
		"@types/node": "20.16.8",
		"@typescript-eslint/eslint-plugin": "8.39.0",
		"@typescript-eslint/parser": "8.39.0",
		"dotenv": "16.4.5",
		"envalid": "^8.0.0",
		"eslint": "9.32.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-prettier": "5.5.3",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"prettier": "3.6.2",
		"typescript": "5.6.3",
		"vitest": "1.6.0"
	}
}
